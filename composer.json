{
    "name": "wterberg/dcat-ap-donl",
    "description": "Allows for creation and validation of datasets conforming to the DCAT-AP-DONL 1.1 metadatastandard",
    "version": "1.0.0-dev",
    "type": "library",
    "keywords": ["DCAT", "DCAT-AP-DONL", "DONL", "Dataset", "KOOP", "Metadata", "Standard", "NL", "Nederland"],
    "homepage": "https://github.com/WterBerg/php-dcatapdonl",
    "readme": "README.md",
    "license": "MIT",
    "abandoned": false,
    "authors": [
        {
            "name": "Willem ter Berg",
            "email": "wrpterberg@gmail.com",
            "homepage": "https://github.com/WterBerg",
            "role": "Developer"
        }
    ],
    "support": {
        "issues": "https://github.com/WterBerg/php-dcatapdonl/issues",
        "source": "https://github.com/WterBerg/php-dcatapdonl",
        "docs": "https://github.com/WterBerg/php-dcatapdonl/blob/master/README.md"
    },
    "autoload": {
        "psr-4": {
            "DCAT_AP_DONL\\ComplexEntity\\": "src/ComplexEntity",
            "DCAT_AP_DONL\\": "src/",
            "": "src/"
        },
        "classmap": ["src/"],
        "exclude-from-classmap": ["test/", "docs/"]
    },
    "scripts": {
        "test": "phpunit --bootstrap vendor/autoload.php --testdox test/"
    },
    "require": {
        "php": "^7.2",
        "ext-json": "*",
        "ext-curl": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^7"
    }
}